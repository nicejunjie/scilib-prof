void zrot_(const int* n, const double complex* x, const int* incx, const double complex* y, const int* incy, const double* c, const double complex* s);

# Modified Givens Rotation (Apply modified Givens rotation)
void srotm_(const int* n, float* x, const int* incx, float* y, const int* incy, const float* param);
void drotm_(const int* n, double* x, const int* incx, double* y, const int* incy, const double* param);

# Modified Givens Rotation (Construct modified Givens rotation)
void srotmg_(float* d1, float* d2, float* b1, const float* b2, float* param);
void drotmg_(double* d1, double* d2, double* b1, const double* b2, double* param);

